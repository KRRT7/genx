   Merte, Lindsay Richard/0000-0002-3213-4199},
Unique-ID = {WOS:000720143000005},
}

@article{ WOS:000687837700001,
Author = {Magnus, F. and Arnalds, U. B. and Palonen, H. and Palsson, G. K. and

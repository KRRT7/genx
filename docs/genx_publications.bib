@InProceedings{Singh2016,
  author               = {Singh, Surendra and Basu, Saibal},
  booktitle            = {{DAE SOLID STATE PHYSICS SYMPOSIUM 2015}},
  title                = {{Simultaneous Parameter Optimization of X-ray and Neutron Reflectivity Data Using Genetic Algorithms}},
  year                 = {{2016}},
  editor               = {{Chitra, R and Bhattacharya, S and Sahoo, NK}},
  note                 = {{DAE Solid State Physics Symposium, Amity Univ, Noida, INDIA, DEC 21-25, 2015}},
  organization         = {{Board Res Nucl Sci, Dept Atom Energy; Dept Atom Energy}},
  series               = {{AIP Conference Proceedings}},
  volume               = {{1731}},
  abstract             = {{X-ray and neutron reflectivity are two non destructive techniques which
   provide a wealth of information on thickness, structure and interracial
   properties in nanometer length scale. Combination of X-ray and neutron
   reflectivity is well suited for obtaining physical parameters of
   nanostructured thin films and superlattices. Neutrons provide a
   different contrast between the elements than X-rays and are also
   sensitive to the magnetization depth profile in thin films and
   superlattices. The real space information is extracted by fitting a
   model for the structure of the thin film sample in reflectometry
   experiments. We have applied a Genetic Algorithms technique to extract
   depth dependent structure and magnetic in thin film and multilayer
   systems by simultaneously fitting X-ray and neutron reflectivity data.}},
  article-number       = {{080007}},
  doi                  = {{10.1063/1.4947885}},

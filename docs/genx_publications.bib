Author = {Podkaminer, J. P. and Hernandez, T. and Huang, M. and Ryu, S. and Bark,
   C. W. and Baek, S. H. and Frederick, J. C. and Kim, T. H. and Cho, K. H.
   and Levy, J. and Rzchowski, M. S. and Eom, C. B.},
Title = {Creation of a two-dimensional electron gas and conductivity switching of
   nanowires at the LaAlO3/SrTiO3 interface grown by 90 degrees off-axis
   sputtering},
Journal = {APPLIED PHYSICS LETTERS},
Year = {2013},
Volume = {103},
Number = {7},
Month = {AUG 12},
Abstract = {Two-dimensional electron gas (2DEG) formed at the interface between two
   oxide band-insulators LaAlO3 and SrTiO3 raises the possibility to
   develop oxide nanoelectronics. Here, we report the creation of a 2DEG at
   the LaAlO3/SrTiO3 heterointerfaces grown by 90 degrees off-axis
   sputtering which allows uniform films over a large area. The electrical
   transport properties of the LaAlO3/SrTiO3 heterointerface are similar to
   those grown by pulsed laser deposition. We also demonstrate
   room-temperature conductive probe-based switching of
   quasi-one-dimensional structures. This work demonstrates that a scalable
   growth process can be used to create the two-dimensional electron gas
   system at oxide heterointerfaces. (C) 2013 AIP Publishing LLC.},

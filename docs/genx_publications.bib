@article{ WOS:000313423900004,
Author = {Garcia-Barriocanal, J. and Kobrinskii, A. and Leng, X. and Kinney, J.
   and Yang, B. and Snyder, S. and Goldman, A. M.},
Title = {Electronically driven superconductor-insulator transition in
   electrostatically doped La2CuO4+delta thin films},
Journal = {PHYSICAL REVIEW B},
Year = {2013},
Volume = {87},
Number = {2},
Month = {JAN 14},
Abstract = {Using an electronic double layer transistor we have systematically
   studied the superconductor-to-insulator transition in La2CuO4+delta thin
   films grown by ozone-assisted molecular-beam epitaxy. We have confirmed
   the high crystalline quality of the cuprate films and have demonstrated
   the suitability of the electronic double layer technique to continuously
   vary the charge density in a system that is otherwise characterized by
   the presence of miscibility gaps. The transport and magnetotransport
   results highlight the role of electron-electron interactions in the
   mechanism of the transition due to the proximity of the Mott-insulating
   state.},

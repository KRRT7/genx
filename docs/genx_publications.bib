@Article{Kc2017,
  author               = {Kc, Amit and Borisov, Pavel and Shvartsman, Vladimir V. and Lederman, David},
  journal              = {{APPLIED PHYSICS LETTERS}},
  title                = {{Weak ferromagnetism and short range polar order in NaMnF3 thin films}},
  year                 = {{2017}},
  issn                 = {{0003-6951}},
  month                = {{FEB 27}},
  number               = {{9}},
  volume               = {{110}},
  abstract             = {{The orthorhombically distorted perovskite NaMnF3 has been predicted to
   become ferroelectric if an a = c distortion of the bulk Pnma structure
   is imposed. In order to test this prediction, NaMnF3 thin films were
   grown on SrTiO3 (001) single crystal substrates via molecular beam
   epitaxy. The best films were smooth and single phase with four different
   twin domains. In-plane magnetization measurements revealed the presence
   of antiferromagnetic ordering with weak ferromagnetism below the Neel
   temperature T-N = 66 K. For the dielectric studies, NaMnF3 films were
   grown on a 30 nm SrRuO3 (001) layer used as a bottom electrode grown via
   pulsed laser deposition. The complex permittivity as a function of
   frequency indicated a strong Debye-like relaxation contribution
   characterized by a distribution of relaxation times. A power-law
   divergence of the characteristic relaxation time revealed an
   order-disorder phase transition at 8 K. The slow relaxation dynamics
   indicated the formation of superdipoles (superparaelectric moments) that
   extend over several unit cells, similar to polar nanoregions of relaxor
   ferroelectrics. Published by AIP Publishing.}},
  article-number       = {{092901}},
  doi                  = {{10.1063/1.4977421}},
  eissn                = {{1077-3118}},
  orcid-numbers        = {{Lederman, David/0000-0001-8423-5138 Borisov, Pavel/0000-0003-1464-6999 Lederman, David/0000-0001-8423-5138 Shvartsman, Vladimir/0000-0002-7155-2473}},
  researcherid-numbers = {{Lederman, David/AAX-6116-2020 Borisov, Pavel/A-6218-2015 Lederman, David/AAX-6120-2020 Shvartsman, Vladimir/J-4210-2014}},
  unique-id            = {{ISI:000397871600041}},
}

@Article{McBriarty2017,
  author               = {McBriarty, Martin E. and von Rudorff, Guido Falk and Stubbs, Joanne E. and Eng, Peter J. and Blumberger, Jochen and Rosso, Kevin M.},
  journal              = {{JOURNAL OF THE AMERICAN CHEMICAL SOCIETY}},
  title                = {{Dynamic Stabilization of Metal Oxide-Water Interfaces}},
  year                 = {{2017}},
  issn                 = {{0002-7863}},
  month                = {{FEB 22}},
  number               = {{7}},
  pages                = {{2581-2584}},
  volume               = {{139}},
  abstract             = {{The interaction of water with metal oxide surfaces plays a crucial role
   in the catalytic and geochemical behavior of metal oxides. In a vast
   majority of studies, the interfacial structure is assumed to arise from
   a relatively static lowest energy configuration of atoms, even at room
   temperature. Using hematite (alpha-Fe2O3) as a model oxide, we show
   through a direct comparison of in situ synchrotron X-ray scattering with
   density functional theory-based molecular dynamics simulations that the
   structure of the (1 (1) over bar 02) termination is dynamically
   stabilized by picosecond water exchange. Simulations show frequent
   exchanges between terminal aquo groups and adsorbed water in locations
   and with partial residence times consistent with experimentally
   determined atomic sites and fractional occupancies. Frequent water
   exchange occurs even for an ultrathin adsorbed water film persisting on
   the surface under a dry atmosphere. The resulting time averaged
   interfacial structure consists of a ridged lateral arrangement of
   adsorbed water molecules hydrogen bonded to terminal aquo groups.
   Surface pK(a) prediction based on bond valence analysis suggests that
   water exchange will influence the proton-transfer reactions underlying
   the acid/base reactivity at the interface. Our findings provide
   important new insights for understanding complex interfacial chemical
   processes at metal oxide water interfaces.}},
  doi                  = {{10.1021/jacs.6b13096}},
  orcid-numbers        = {{von Rudorff, Guido Falk/0000-0001-7987-4330 Stubbs, Joanne E/0000-0002-8509-2009 McBriarty, Martin/0000-0002-7802-3267}},
  researcherid-numbers = {{von Rudorff, Guido Falk/AAA-9693-2020 Stubbs, Joanne E/F-9710-2013 }},
  unique-id            = {{ISI:000394829200012}},
}

@Article{Satchell2017,
  author               = {Satchell, N. and Witt, J. D. S. and Burnell, G. and Curran, P. J. and Kinane, C. J. and Charlton, T. R. and Langridge, S. and Cooper, J. F. K.},
  journal              = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
  title                = {{Probing the spiral magnetic phase in 6 nm textured erbium using polarised neutron reflectometry}},
  year                 = {{2017}},
  issn                 = {{0953-8984}},
  month                = {{FEB 8}},
  number               = {{5}},
  volume               = {{29}},
  abstract             = {{We characterise the magnetic state of highly-textured, sputter deposited
   erbium for a film of thickness 6 nm. Using polarised neutron
   reflectometry it is found that the film has a high degree of magnetic
   disorder, and we present some evidence that the film's local magnetic
   state is consistent with bulk-like spiral magnetism. This, combined with
   complementary characterisation techniques, show that thin film erbium is
   a strong candidate material for incorporation into device structures.}},
  article-number       = {{055801}},
  doi                  = {{10.1088/1361-648X/29/5/055801}},

}

@article{ WOS:000436226800140,
Author = {Mukhtarova, Anna and Redaelli, Luca and Hazra, Dibyendu and Machhadani,
   Houssaine and Lequien, Stephane and Hofheinz, Max and Thomassin,
   Jean-Luc and Gustavo, Frederic and Zichi, Julien and Zwiller, Val and
   Monroy, Eva and Gerard, Jean-Michel},
Title = {Polarization-insensitive fiber-coupled superconducting-nanowire single
   photon detector using a high-index dielectric capping layer},
Journal = {OPTICS EXPRESS},
Year = {2018},
Volume = {26},
Number = {13},
Pages = {17697-17704},
Month = {JUN 25},
Abstract = {Superconducting-nanowire single photon detectors (SNSPDs) are able to
   reach near-unity detection efficiency in the infrared spectral range.
   However, due to the intrinsic asymmetry of nanowires, SNSPDs are usually
   very sensitive to the polarization of the incident radiation, their
   responsivity being maximum for light polarized parallel to the nanowire
   length (transverse-electric (TE) polarization). Here, we report on the

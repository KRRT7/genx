  year     = {2020},
  month    = dec,
  number   = {12},
  pages    = {121103},
  volume   = {8},
  af       = {Griggs, W.EOLEOLEggert, B.EOLEOLLiedke, M. O.EOLEOLButterling, M.EOLEOLWagner, A.EOLEOLKentsch, U.EOLEOLHirschmann, E.EOLEOLGrimes, M.EOLEOLCaruana, A. J.EOLEOLKinane, C.EOLEOLWende, H.EOLEOLBali, R.EOLEOLThomson, T.},

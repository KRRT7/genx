}

@article{ WOS:000359376700038,
Author = {Ribera, R. Coloma and van de Kruijs, R. W. E. and Yakshin, A. E. and
   Bijkerk, F.},
Title = {Determination of oxygen diffusion kinetics during thin film ruthenium
   oxidation},
Journal = {JOURNAL OF APPLIED PHYSICS},
Year = {2015},
Volume = {118},
Number = {5},
Month = {AUG 7},
Abstract = {In situ X-ray reflectivity was used to reveal oxygen diffusion kinetics
   for thermal oxidation of poly-crystalline ruthenium thin films and
   accurate determination of activation energies for this process.
   Diffusion rates in nanometer thin RuO2 films were found to show
   Arrhenius behaviour. However, a gradual decrease in diffusion rates was
   observed with oxide growth, with the activation energy increasing from
   about 2.1 to 2.4 eV. Further exploration of the Arrhenius
   pre-exponential factor for diffusion process revealed that oxidation of
   polycrystalline ruthenium joins the class of materials that obey the
   Meyer-Neldel rule. (C) 2015 AIP Publishing LLC.},
DOI = {10.1063/1.4928295},
Article-Number = {055303},

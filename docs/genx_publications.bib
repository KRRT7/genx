@article{ WOS:000414133800002,
Author = {Li, Mingda and Song, Qichen and Zhao, Weiwei and Garlow, Joseph A. and
   Liu, Te-Huan and Wu, Lijun and Zhu, Yimei and Moodera, Jagadeesh S. and
   Chan, Moses H. W. and Chen, Gang and Chang, Cui-Zu},
Title = {Dirac-electron-mediated magnetic proximity effect in topological
   insulator/magnetic insulator heterostructures},
Journal = {PHYSICAL REVIEW B},
Year = {2017},
Volume = {96},
Number = {20},
Month = {NOV 1},
Abstract = {The possible realization of dissipationless chiral edge current in a
   topological insulator/magnetic insulator heterostructure is based on the
   condition that the magnetic proximity exchange coupling at the interface
   is dominated by the Dirac surface states of the topological insulator.
   Here we report a polarized neutron reflectometry observation of
   Dirac-electron-mediated magnetic proximity effect in a bulk-insulating
   topological insulator (Bi0.2Sb0.8)(2)Te-3/magnetic insulator EuS
   heterostructure. We are able tomaximize the proximity-induced magnetism
   by applying an electrical back gate to tune the Fermi level of
   topological insulator to be close to the Dirac point. A phenomenological
   model based on diamagnetic screening is developed to explain the
   suppressed proximity-induced magnetism at high carrier density. Our work
   paves the way to utilize the magnetic proximity effect at the
   topological insulator/magnetic insulator heterointerface for low-power
   spintronic applications.},
DOI = {10.1103/PhysRevB.96.201301},
Article-Number = {201301},

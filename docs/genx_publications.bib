@Article{Andersson2006,
  author    = {G Andersson and M Björck and H Lidbaum and B Sanyal and C Chacon and C Zlotea and S Valizadeh},
  journal   = {Journal of Physics: Condensed Matter},
  title     = {Structure of Fe{\textendash}Co/Pt(001) superlattices: a realization of tetragonal Fe{\textendash}Co alloys},
  year      = {2006},
  month     = {dec},
  number    = {1},
  pages     = {016008},
  volume    = {19},
  abstract  = {The structural properties of a tetragonally distorted Fe1−xCox alloy, in the form of Fe1−xCox/Pt(001) superlattices with x = 0.64, have been investigated experimentally. The study follows recent theoretical predictions on the enhanced uniaxial magnetocrystalline anisotropy of such alloys with specific combinations of chemical composition and tetragonal distortion. The ratio between out-of-plane and in-plane lattice parameters in the Fe0.36Co0.64 layers, c/a, was found to vary between 1.18 and 1.31, depending on the thickness ratio between the alloy and the spacer. This covered the range of interest c/a = 1.20–1.25 in the previous calculations and should be compared to c/a = 1 in the original bcc alloy lattice. Simulations of x-ray diffraction patterns as well as density functional calculations support the derivation of the Fe0.36Co0.64 lattice parameters.},
  doi       = {10.1088/0953-8984/19/1/016008},
  publisher = {{IOP} Publishing},
  url       = {https://doi.org/10.1088%2F0953-8984%2F19%2F1%2F016008},
}
